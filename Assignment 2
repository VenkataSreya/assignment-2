e={1:['sreya','supervisor',30000,'kakinada'],2:['sahithi','account officer',45000,'banglore'],3:['rashi','sweeper',12000,'hyderabad']}

x=int(input('enter the employee number:'))

print('name of the employee:',e[x][0])

print('employee designation:',e[x][1])

print('employee salary:',e[x][2])

print('employee city:',e[x][3])
